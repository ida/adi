<div class="fieldset times" metal:define-macro="times">
  <div class="fieldpair age">
    <div class="name" tal:content="string:Age">
      Age
    </div><div class="value">
      <tal:rep tal:repeat="a age">
        <span tal:content="a">
        </span>
      </tal:rep>
    </div>
  </div><div class="fieldpair activity">
    <div class="name" tal:content="string:Activity">
      Activity
    </div><div class="value">
      <tal:rep tal:repeat="a activity">
        <span tal:content="a">
        </span>
      </tal:rep>
    </div>
  </div><div class="fieldpair total_activity">
    <div class="name" tal:content="string:Activities">
      Activities
    </div><div class="value">
      <tal:rep tal:repeat="a total_activity">
        <span tal:content="a">
        </span>
      </tal:rep>
    </div>
  </div>
</div>
